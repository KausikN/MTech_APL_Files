#include "../include/Complex.h"
#include <iostream>
/*-------------------------------------------------------------------------
 *  main -- Main Program for Complex
 *    Args:	none
 *    Returns:	int
 *    Throws:	
 *    See:	
 *    Bugs:	
 -------------------------------------------------------------------------*/
int main()
{
  Complex a(0.5,0.5), b(2.5,2.5), c(0.0,0.0), d;
 int x, y, z;
 c = a + b;
 x = 4;
 y = 6;
 z = x + y;
 c.print();
 cout << "zsum = " << z << endl;
 d = a*b;
 d.print();
 return 0;
}	/*  End of main		End of main   */



